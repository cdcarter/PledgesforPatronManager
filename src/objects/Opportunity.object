    <validationRules>
        <fullName>Valid_CC_Number</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISNULL(PatronDonate__CC_Number__c),PatronDonate__CC_Number__c==&apos;&apos;,REGEX(PatronDonate__CC_Number__c,&quot;(((\\d{4}-){3}\\d{4})|\\d{16})?&quot;)))</errorConditionFormula>
        <errorDisplayField>PatronDonate__CC_Number__c</errorDisplayField>
        <errorMessage>The credit card number you have entered is not valid.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Expiration_Month</fullName>
        <active>false</active>
        <description>Validates the credit card expiration month.</description>
        <errorConditionFormula>NOT(OR(ISNULL(PatronDonate__Expiration_Month__c),PatronDonate__Expiration_Month__c==&apos;&apos;,REGEX( PatronDonate__Expiration_Month__c , &quot;([1-9]|0[1-9]|1[012])&quot;)))</errorConditionFormula>
        <errorDisplayField>PatronDonate__Expiration_Month__c</errorDisplayField>
        <errorMessage>The credit card expiration month must be blank, or an integer value between 1 and 12, inclusive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Expiration_Year</fullName>
        <active>true</active>
        <description>Validates the credit card expiration year. Year must be between 2008 and 2050.</description>
        <errorConditionFormula>NOT(OR(ISNULL(PatronDonate__Expiration_Year__c),PatronDonate__Expiration_Year__c==&apos;&apos;,REGEX(PatronDonate__Expiration_Year__c, &quot;(20[0-9][0-9])&quot;)))</errorConditionFormula>
        <errorDisplayField>PatronDonate__Expiration_Year__c</errorDisplayField>
        <errorMessage>The credit card expiration year must be blank, or an integer value between 2000 and 2099, inclusive.</errorMessage>
    </validationRules>
